<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
    <persistence-unit name="MONTEREY">
        <provider>org.hibernate.ejb.HibernatePersistence</provider>
        <class>us.kulba.monterey.model.Contact</class>
        <class>us.kulba.monterey.model.ContactGroup</class>
        <class>us.kulba.monterey.model.Event</class>
        <class>us.kulba.monterey.model.Email</class>
        <class>us.kulba.monterey.model.HomeEmail</class>
        <exclude-unlisted-classes>true</exclude-unlisted-classes>
    </persistence-unit>

</persistence>
